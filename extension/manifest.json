{
  "manifest_version": 3,
  "name": "TabTrack",
  "version": "1.0.0",
  "description": "Track and visualize your browsing habits for better focus.",
  "permissions": ["tabs", "storage", "activeTab", "scripting"],
  "host_permissions": [
    "http://localhost:5000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  }
}
